{
	"name": "flarum-lang/albanian",
	"type": "flarum-extension",
	"description": "Albanian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/albanian",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"sq-locale",
		"albanian-locale",
		"albanian"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/albanian/issues",
		"source": "https://github.com/flarum-lang/albanian"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Albanian",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "sq",
			"title": "Albanian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
